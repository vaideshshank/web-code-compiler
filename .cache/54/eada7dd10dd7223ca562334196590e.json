'cors',\n        body:JSON.stringify({\n            clientId:process.env.COMPILER_CLIENT_ID,\n            clientSecret:process.env.COMPILER_CLIENT_SECRET,\n            script:state.code,\n            language:state.language.key,\n            versionIndex:state.language.versionIndex,\n            stdin:state.input\n        })\n    })\n    .then(res=>res.json())\n    .then(data=>{\n        state.result=data;\n        console.log(state.result);\n        state.running=false;\n    })\n    .catch(e=>{\n        console.log(e);\n        state.running=false;\n    })\n    \n}\n"},"lineCount":null}},"error":null,"hash":"6719a619afd00ece0c2741b3b6451e45","cacheData":{"env":{"COMPILER_CLIENT_ID":"dd77126df2a3df539f341cb2235f9045","COMPILER_CLIENT_SECRET":"222778ca04e53bc8f3bcf8d394b7958202af4153d493d8bf291d5b75e1ef05b3"}}}