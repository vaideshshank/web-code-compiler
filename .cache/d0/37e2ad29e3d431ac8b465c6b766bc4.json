 {\n    width: 4px;\n}\n::-webkit-scrollbar-track {\n    background: #343434;\n}\n::-webkit-scrollbar-thumb {\n    background: #888; \n    border-radius:10px;\n}\n\n.container{\n    padding: 16px;\n}\n\n.container__header{\n    margin: 0;\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    height: 60px;\n}\n\n.container__header-title{\n    font-family: Lucida Grande;\n    font-size: 32px;\n    color: white;\n}\n\n.container__header-options{\n    display: flex;\n}\n\n.container__header-option .select{\n    background-color: #07073B;\n    color: white;\n    font-size: 20px;\n    padding: 8px 16px;\n    border-radius: 8px;\n    margin-left: 10px;\n}\n\n.editor{\n    display: flex;\n    height: calc(100vh - 100px);\n    font-family: Arial, Helvetica, sans-serif;\n}\n\n.editor .editor__components{\n    display: flex;\n    flex-direction: column;\n    /* row-gap: 16px; */\n    width: 50%;\n}\n\n.editor .editor__components .editor__component{\n    background-color: #07073B;\n    border: 4px solid #95ABD5;\n}\n\n.editor .editor__component .editor__input-input,\n.editor .editor__component .editor__output-output{\n    padding: 10px;\n    height: calc(100% - 50px);\n    color: white;\n    font-size: 20px;\n    font-family: Arial;\n    line-height: 28px;\n    outline: none;\n    white-space: pre-wrap;\n    overflow: auto;\n}\n\n\n.editor__components .editor__code{\n    height: 90%;\n}\n\n.editor__components .editor__action{\n    height: 10%;\n    display: flex;\n    align-items: center;\n    padding: 10px;\n}\n\n.editor__components .editor__input,\n.editor__components .editor__output{\n    height: 50%;\n}\n\n\n.editor__component .editor__component-title{\n    background-color: rgba(0, 0, 0, 0.64);\n    width: calc(100% - 8px);\n    color: white;\n    font-family: Arial;\n    font-size: 20px;\n    padding: 4px;\n    user-select: none;\n    height: 22px;\n}\n\n.editor__component .execute{\n    background-color: #212161;\n    color: white;\n    padding: 10px 20px;\n    font-size: 20px;\n    border: 2px solid #333376;\n    cursor: pointer;\n}\n\n.editor__component .execute:hover{\n    opacity: 0.9;\n}\n\n.editor__component .process-results{\n    margin-left: 16px;\n}\n\n.editor__component .process-results .cpu-time{\n    color: red;\n}\n\n.editor__component .process-results .memory{\n    color: green;\n}\n\n.editor .resize.col{\n    display: flex;\n    align-items: center;\n    width: 26px;\n    border-radius: 32px;\n}\n\n.editor .resize.col:hover{\n    background-color: rgba(0, 0, 0, 0.13);\n    cursor: col-resize;\n}\n\n.editor .resize.col img{\n    width: 100%;\n    transform: rotate(90deg);  \n    user-select: none; \n    -webkit-user-drag: none;\n}\n\n.editor .resize.row{\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    height: 26px;\n    border-radius: 32px;\n}\n.editor .resize.row img{\n    height: 38%;\n    user-select: none;\n    -webkit-user-drag: none;\n}\n\n.editor .resize.row:hover{\n    background-color: rgba(0, 0, 0, 0.13);\n    cursor: row-resize;\n}\n\n\n#editor{\n    height: calc(100% - 30px);\n    cursor: text;\n}\n\n#editor>.CodeMirror{\n    height: 100%;\n}"},"lineCount":null}},"error":null,"hash":"fc56b0eb2319e296a6ae81009ca81e64","cacheData":{"env":{}}}